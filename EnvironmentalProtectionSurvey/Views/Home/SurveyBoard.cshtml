@model EnvironmentalProtectionSurvey.Models.NewsSurveyViewModel
@using EnvironmentalProtectionSurvey.Models
@using Microsoft.AspNetCore.Http
@{
    ViewData["Title"] = "Enviromental Protection";
}
<!DOCTYPE html>
<html lang="">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="icon" href="https://fuvavi.com/wp-content/themes/fuvavi-v2/favicon.png">
    <link rel="stylesheet" type="text/css" href="https://fuvavi.com/wp-content/themes/fuvavi-v2/css/style.css">
    <link rel="stylesheet" type="text/css" href="https://fuvavi.com/wp-content/themes/fuvavi-v2/style.css">
    <!-- / Yoast SEO plugin. -->
</head>

<body>
    <div class="wrapper">
        <!-- HEADER -->
        <header id="header" class="f-header">
            <nav class="navbar navbar-expand-md fixed-top">
                <div class="container">
                    <div class="f-logo">hello world</div>
                </div>
            </nav>
        </header>
        <!-- !HEADER -->
        <div class="f-content">
            <div class="container">
                <div class="flex-xl-nowrap">
                    <div class="fvv-content single-content">
                        <div class="f-post-content-box">
                            <p id="breadcrumbs" class="f-breadcrumbs text-left"><span><span><a>Trang chủ</a></span> » <span class="breadcrumb_last" aria-current="page">BEM: phương pháp tổ chức CSS hiệu quả và linh hoạt nhất</span></span></p>                		<div class="f-post-img">
                                <img src="https://fuvavi.com/wp-content/uploads/2023/06/fuvavi-bem.jpg" class="attachment-full size-full wp-post-image" alt="BEM: phương pháp tổ chức CSS hiệu quả và linh hoạt nhất" decoding="async" loading="lazy" />                            <a href="#" class="f_zoom_avatar f_zoom_avatar_btn">
                                    <i class="fa fa-play" aria-hidden="true"></i>
                                    <div class="f-post-category">
                                        <div class="f-post-category-link">
                                            <ul class="post-categories">
                                                <li><a href="https://fuvavi.com/category/thiet-ke-web" rel="category tag">Thiết kế web</a></li>
                                            </ul>
                                        </div>
                                        <div class="f-post-zoom-avatar f_zoom_avatar_btn">
                                            <img src="https://fuvavi.com/wp-content/themes/fuvavi-v2/images/icon/ic_expand.png" alt="Phóng to" title="Phóng to">
                                        </div>
                                    </div>
                            </div>
                            <div class="f-post-content-left">
                                <div class="f-post-title">
                                    <h1>BEM: phương pháp tổ chức CSS hiệu quả và linh hoạt nhất</h1>
                                </div>
                            </div>
                            @{
                                var index = 0;
                            }

                            <div class="f-post-content-right">
                                <div class="fvv-content-box">
                                    <h3 class="fcb-title">Tiếp theo</h3>
                                    @foreach (var survey in Model.SurveyList)
                                    {
                                        <div class="post_box post_box_related">
                                            <div class="post_box_item">
                                                <div class="post_box_content">
                                                    <h3 class="post_box_title">
                                                        @survey.Title
                                                    </h3>
                                                </div>
                                                @if (Context.Session.GetString("username") == null)
                                                {
                                                    <p>Please <a href="@Url.Action("Login", "Users")">login</a> to participate in this survey.</p>
                                                }
                                                else
                                                {
                                                    <h3>
                                                        <a href="@Url.Action("TakeSurvey", "Home", new { id = survey.Id })">More Details &raquo;</a>
                                                    </h3>
                                                }
                                                <div class="post_box_media">
                                                    <div class="post_view">Start: @survey.CreatedAt</div>
                                                    <span class="post_dot">•</span>
                                                    <div class="post_time">End: @survey.EndAt</div>
                                                </div>
                                            </div>
                                        </div>
                                        index++;
                                    }
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <script type="text/javascript" src="https://fuvavi.com/wp-content/themes/fuvavi-v2/js/jquery-3.2.1.min.js"></script>
        <script type="text/javascript" src="https://fuvavi.com/wp-content/themes/fuvavi-v2/libs/popper/umd/popper.js"></script>
        <script type="text/javascript" src="https://fuvavi.com/wp-content/themes/fuvavi-v2/libs/bootstrap/js/bootstrap.min.js"></script>
        <script type="text/javascript" src="https://fuvavi.com/wp-content/themes/fuvavi-v2/libs/owlcarousel/owl.carousel.min.js"></script>
        <script type="text/javascript" src="https://fuvavi.com/wp-content/themes/fuvavi-v2/js/main.js"></script>
        
        <script type="text/javascript" src="https://fuvavi.com/wp-content/plugins/wpdiscuz/assets/js/wpdiscuz-combo.min.js?ver=7.6.13" id="wpdiscuz-combo-js-js"></script>
        <script type="text/javascript" id="ez-toc-scroll-scriptjs-js-extra">
            /* <![CDATA[ */
            var eztoc_smooth_local = { "scroll_offset": "30" };
            /* ]]> */
        </script>
