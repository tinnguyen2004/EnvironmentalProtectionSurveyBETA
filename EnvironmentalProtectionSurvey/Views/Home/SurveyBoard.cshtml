        <script type="text/javascript">
            jQuery(document).ready(function () {
                $('.channel-menu-inner ul li').last().click(function () {
                    $('.f-navbar-search-mb').slideDown();
                    $('#nav-input-search').focus();
                });
            });
        </script>
</body>
</html>
<script type="text/javascript">
    jQuery(document).ready(function () {
        $('.f-post-content img').attr('data-action', 'zoom');
    });
</script>
<link rel="stylesheet" type="text/css" href="https://fuvavi.com/wp-content/themes/fuvavi-v2/libs/zoomimgpost/css/zoom.css">
<script type="text/javascript" src="https://fuvavi.com/wp-content/themes/fuvavi-v2/libs/zoomimgpost/js/velocity.min.js"></script>
<script type="text/javascript" src="https://fuvavi.com/wp-content/themes/fuvavi-v2/libs/zoomimgpost/js/enhance.js"></script>
<script type="text/javascript">
    $('.fvv-sidebar').toggleClass('hidden-menu');
    $('.f-content .fvv-content').toggleClass('hidden-menu');
    $('#btn-menu-pc').click(function () {
        $('.fvv-sidebar').toggleClass('hidden-menu');
        if ($('.fvv-sidebar').hasClass('hidden-menu')) {
            $('.backdrop-menu').removeClass('active');
        }
        else {
            $('.backdrop-menu').addClass('active');
            $('.backdrop-menu').click(function () {
                $('.backdrop-menu').removeClass('active');
                $('.fvv-sidebar').addClass('hidden-menu');
            });
        }
    });
</script>
